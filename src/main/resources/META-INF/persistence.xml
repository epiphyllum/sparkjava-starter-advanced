<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="defaultPersistenceUnit" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>com.thoughtlogix.advancedstarter.models.core.FileData</class>
        <class>com.thoughtlogix.advancedstarter.models.core.FileInfo</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Log</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Recent</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Starred</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Profile</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Scope</class>
        <class>com.thoughtlogix.advancedstarter.models.core.Server</class>
        <class>com.thoughtlogix.advancedstarter.models.core.SystemLock</class>
        <class>com.thoughtlogix.advancedstarter.models.users.Role</class>
        <class>com.thoughtlogix.advancedstarter.models.users.Permission</class>
        <class>com.thoughtlogix.advancedstarter.models.users.Team</class>
        <class>com.thoughtlogix.advancedstarter.models.users.User</class>
        <class>com.thoughtlogix.examples.models.Todo</class>
        <properties>
        </properties>
    </persistence-unit>
</persistence>
